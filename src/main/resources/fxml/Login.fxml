<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane id="loginStackPane" prefHeight="179.0" prefWidth="348.0" xmlns="http://javafx.com/javafx/8" 
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mvp.java.controllers.LoginController">
  <children>
  <VBox prefHeight="339.0" prefWidth="330.0" spacing="10.0">
    <children>
            <Label fx:id="statusLabel" alignment="CENTER" prefHeight="22.0" prefWidth="326.0" textFill="RED" />
      <GridPane prefHeight="81.0" prefWidth="256.0">
        <children>
          <Label text="Username:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
          <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
          <TextField fx:id="userField" onMouseClicked="#usernameTextFieldMouseClicked" text="all" GridPane.columnIndex="1" GridPane.rowIndex="0" />
          <TextField fx:id="passwordField" onMouseClicked="#passwordTextFieldMouseClicked" text="all" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        </children>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
            <AnchorPane prefHeight="45.0" prefWidth="330.0">
               <children>
          <Button fx:id="loginButton" alignment="CENTER" defaultButton="true" layoutX="217.0" layoutY="5.0" mnemonicParsing="false" onAction="#loginButtonPressed" prefHeight="21.0" prefWidth="111.0" text="Login" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="5.0" />
               </children>
            </AnchorPane>
    </children>
  </VBox>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</StackPane>
